---
import H1 from "../components/h1.astro";
import InfoLayout from "../layouts/InfoLayout.astro";
import ExternalLink from "../components/external-link.astro";
---

<InfoLayout>
     <div class="p-4 sm:pt-8 sm:px-12 pb-20 min-h-dvh flex flex-col justify-between">
          
          <div>
               <H1 >How I can help? </H1>
               <p class="mt-4 md:mt-8 lg:mt-12 text-xl xl:text-2xl text-zinc-400 text-balance">
                    Interested in working together?
                    Get in touch for new projects and experiments.
               </p>

               <div class="mt-6 sm:mt-12 flex flex-col gap-1">
                    <div data-tf-live="01KA3DWX3JZ1K964TJTVZ0RSDA"></div><script src="//embed.typeform.com/next/embed.js" is:inline></script>

                    <ExternalLink href="https://x.com/intent/user?screen_name=arinaspb">Twitter</ExternalLink>
                    <ExternalLink href="https://instagram.com/">Instagram</ExternalLink>
                    <ExternalLink href="https://www.youtube.com/watch?v=dQw4w9WgXcQ">YouTube</ExternalLink>
                    <ExternalLink href="https://github.com/arinaspb/">GitHub</ExternalLink>
               </div>
          </div>

          <div>
               <p class="text-base text-zinc-500">9 Buckley Road</p>
               <p class="text-base text-zinc-500">Singapore 309767</p>
          </div>
     </div>
</InfoLayout>
